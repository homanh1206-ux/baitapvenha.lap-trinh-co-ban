
#include <iostream>
using namespace std;

int main() {
    float dai1, rong1, dai2, rong2;
    float dt1, dt2;

    // Nhập kích thước hình chữ nhật 1
    cout << "Nhap chieu dai hinh 1: ";
    cin >> dai1;
    cout << "Nhap chieu rong hinh 1: ";
    cin >> rong1;

    // Nhập kích thước hình chữ nhật 2
    cout << "Nhap chieu dai hinh 2: ";
    cin >> dai2;
    cout << "Nhap chieu rong hinh 2: ";
    cin >> rong2;

    // Tính diện tích
    dt1 = dai1 * rong1;
    dt2 = dai2 * rong2;

    // So sánh diện tích
    if (dt1 > dt2) {
        cout << "Hinh chu nhat 1 co dien tich lon hon";
    } else if (dt1 < dt2) {
        cout << "Hinh chu nhat 2 co dien tich lon hon";
    } else {
        cout << "Hai hinh chu nhat co dien tich bang nhau";
    }

    return 0;
}
