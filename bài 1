#include <iostream>
using namespace std;

int main() {
    int a, b;

    cout << "Nhap so a: ";
    cin >> a;
    cout << "Nhap so b: ";
    cin >> b;

    int max = (a > b) ? a : b;
    int min = (a < b) ? a : b;

    cout << "So lon hon (Max) = " << max << endl;
    cout << "So nho hon (Min) = " << min << endl;

    return 0;
}
